{
  "environments": [
    {
      "id": "production",
      "name": "Production",
      "region": "us-east-2",
      "source": {
        "branch": "main"
      },
      "services": [
        {
          "id": "my-webapp",
          "name": "my-webapp",
          "type": "fargate",
          "cpu": 0.5,
          "memory": 1024,
          "domain": "beta.feathercrm.io",
          "minInstances": 1,
          "maxInstances": 1,
          "buildCommand": "npm run build",
          "startCommand": "npm run start",
          "envVariables": {
            "APP_ENV": "production",
            "DATABASE_URL": {
              "fromParameterStore": "production.database-url"
            },
            "SESSION_SECRET_KEY": {
              "fromParameterStore": "production.session-secret-key"
            }
          }
        }
      ]
    }
  ]
}
